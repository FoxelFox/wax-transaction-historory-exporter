{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false,
		"moduleResolution": "Node",
		"outDir": "dist",
		"noImplicitAny": false
	},
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Recommended"
}
